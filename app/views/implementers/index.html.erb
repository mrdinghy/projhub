
<section>
  <!-- Page content-->
  <div class="content-wrapper">

    <div class="content-heading">
      <!-- START Language list-->

      <!-- END Language list    -->
      Implementing Organizations
      <small data-localize="dashboard.WELCOME"></small>
    </div>




    <p id="notice"><%= notice %></p>
    <table>
      <thead>
      <tr>
        <th>Name</th>
        <th>Description</th>
        <th colspan="3"></th>
      </tr>
      </thead>

      <tbody>
      <% @implementers.each do |implementer| %>
          <tr>
            <td><%= implementer.name %></td>
            <td><%= implementer.description %></td>
            <td><%= link_to 'Show', implementer %></td>
            <td><%= link_to 'Edit', edit_implementer_path(implementer) %></td>
            <td><%= link_to 'Destroy', implementer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
    <br>

    <%= link_to 'New Implementer', new_implementer_path %>





  </div>
</section>
